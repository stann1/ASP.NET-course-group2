@model IList<TeamMember>

<h1>@ViewData["title"]</h1>

<div>
    <a asp-action="Create">Add member</a>
</div>
<table class="table">
    <tr>
        <th>First name</th>
        <th>Last name</th>
        <th>Email</th>
    </tr>

    @foreach (var member in Model)
    {
        <tr>
            <td>@member.FirstName</td>
            <td>@member.LastName</td>
            <td>@member.Email</td>
        </tr>
    }
</table>
